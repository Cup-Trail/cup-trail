{
  "name": "cuptrail",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev:web": "pnpm --filter @cuptrail/web dev",
    "dev:mobile": "pnpm --filter @cuptrail/mobile dev",
    "dev": "pnpm run dev:web",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix:all": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r run clean",
    "install:all": "pnpm install",
    "upgrade:all": "pnpm -r up --latest",
    "upgrade:check": "pnpm -r up --interactive --latest",
    "upgrade:web": "pnpm --filter @cuptrail/web up --latest"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
