{
  "name": "cuptrail",
  "private": true,
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r run build",
    "dev:web": "pnpm --filter @cuptrail/web dev",
    "dev:mobile": "pnpm --filter @cuptrail/mobile dev",
    "dev": "pnpm run dev:web",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix:all": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r run clean",
    "install:all": "pnpm install",
    "upgrade:all": "pnpm -r up --latest",
    "upgrade:check": "pnpm -r up --interactive --latest",
    "upgrade:web": "pnpm --filter @cuptrail/web up --latest"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/react": "~19.2.14",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  }
}
