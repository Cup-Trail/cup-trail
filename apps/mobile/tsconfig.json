{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "types": ["react-native"],
    "strict": true,
    "skipLibCheck": true,
    "moduleSuffixes": [".native", ""]
  },
  "include": ["src", "src", "index.tsx", "global.d.ts"],
  "paths": {
    "@cuptrail/data/*": ["packages/data/*"]
  }
}
